class Solution:
    def findMaxLength(self, nums: List[int]) -> int:
        dic={}
        dic[0]=-1
        maxlen=0
        count=0
        for i in range(len(nums)):
            if nums[i]==1:
                count+=1
            else:
                count+=-1
            if count in dic:
                maxlen=max(maxlen,i-dic[count])
            else:
                dic[count]=i
        return maxlen
